<!-- Profile Page , Modal -->
<div class="page-header" style="border-bottom: 1px solid white;"></div>
<div class="profile-container">
  <div class="row">
  <div class="col-md-4">
    <% if(list.url!==""){  %> 
        <img src="<%= list.url %>" class="img-responsive" width="200" height="200" />
        <% }else{ %> 
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTEljN4nGwrKt0l4-u4d6lVwm2Jzh9goTiwO4ysLlY-N1vxzLAd42XMIzqQ30gSszi53U8&usqp=CAU" class="img-responsive" width="100" height="100" />
          <% } %> 
  </div>
  <div class="col-md-8 profile-info">
    <h4 class="user-name"><%= list.fname + list.lname%></h4>
    <h5 class="user-mail"><i><%= list.email %></i></h5>
    <h5 class="user-company"><%= list.exp %>+years Experience</h5>
  </div>
</div>
  <br/><br/>
  <div class="row more-info">
  <div class="col-md-6">
    <h5><b>Title</b></h5>
    <p><%= list.title %></p>
  </div>
    <div class="col-md-6">
      <h5><b>Location</b></h5>
    <p><%= list.city %>/<%= list.state %></p>
  </div>
  </div>
   <div class="row more-info">
  <div class="col-md-6">
    <h5><b>Category</b></h5>
    <p><%= list.category %></p> <!-- </3 :( -->
  </div>
    <div class="col-md-6">
      <h5><b>Phone Number</b></h5>
    <p><%= list.phone %></p>
  </div>
  </div>
   <div class="row more-info">
  <div class="col-md-6">
    <h5><b>Gender</b></h5>
    <p><%= list.gender %></p> <!-- </3 :( -->
  </div>
    <div class="col-md-6">
      <h5><b>Price Per Day</b></h5>
    <p><%= list.price %></p>
  </div>
  </div>
   <div class="row more-info">
  <div class="col-md-6">
    <h5><b>Skills</b></h5>
    <p><%= list.skill %></p> <!-- </3 :( -->
  </div>
    <div class="col-md-6">
      <h5><b>Qualification</b></h5>
    <p><%= list.qualification %></p>
  </div>
  </div>
  <br/>
  <div class="row">
    <div class="todo-links"><a href="/kform"><i class="fa fa-pencil-square-o"></i> Edit Info</a>
    </div>

    <div class="todo-links"><a href="/"><i class="fa fa-frown-o"></i>Home</a>
    </div>

  </div>
  <br/><br/>
</div>